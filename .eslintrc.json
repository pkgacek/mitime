{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["eslint-plugin-googleappsscript"],
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": 0,
        "indent": 0,
        "no-plusplus": 0,
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "trailingComma": "all",
                "jsxSingleQuote": false,
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 4,
                "endOfLine": "auto"
            }
        ]
    }
}
